# Excluir archivos de Flutter web, ios, linux y macos de estadísticas
frontend/web/* linguist-generated=true
frontend/ios/* linguist-generated=true
frontend/macos/* linguist-generated=true
frontend/windows/* linguist-generated=true
frontend/linux/* linguist-generated=true

# Lenguajes nativos de plataformas
*.cpp linguist-detectable=false
*.cc linguist-detectable=false
*.c linguist-detectable=false
*.h linguist-detectable=false
*.swift linguist-detectable=false
*.kt linguist-detectable=false
*.java linguist-detectable=false
CMakeLists.txt linguist-detectable=false
*.cmake linguist-detectable=false

# Marcar HTML/CSS/JS como no detectables
*.html linguist-detectable=false
*.css linguist-detectable=false
*.js linguist-detectable=false

# Asegurar que Python y Dart sean detectados
*.py linguist-detectable=true
*.dart linguist-detectable=true

# Marcar archivos de configuración como no detectables
*.json linguist-detectable=false
*.yaml linguist-detectable=false
*.yml linguist-detectable=false
*.toml linguist-detectable=false

# Documentación
*.md linguist-documentation=true

# Otros archivos de configuración comunes
Dockerfile linguist-detectable=false
requirements.txt linguist-detectable=false
.gitignore linguist-detectable=false